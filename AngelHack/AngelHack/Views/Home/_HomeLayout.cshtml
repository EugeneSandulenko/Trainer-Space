<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Spaces for Trainers</title>
    @Styles.Render("~/Content/css")
    <script type="text/javascript" src="https://unpkg.com/vue"></script>
    <script type="text/javascript" src="https://unpkg.com/axios/dist/axios.min.js"></script>

</head>
<body class="navbar-static-top">
    <div class="navbar navbar-custom navbar-fixed-top" style="background-color:white;">
        <div>
            <div class="navbar-header">
                <a href="/">
                    <img src="/Images/Logo.svg" style="height:60px; margin-left:20px;" />
                </a>
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
        </div>
    </div>
    <div >
        @RenderBody()
        <hr />
        <footer style="text-align: right">
            <p>&copy; @DateTime.Now.Year - Trainer Space</p>
        </footer>
    </div>
    @Scripts.Render("~/bundles/bootstrap")
    
</body>
</html>
